{
    "name": "nickpoulos/ez-rider",
    "description": "Ez-Rider: Easily generate Docker Compose Override files with secrets",
    "keywords": ["ez-rider", "docker", "docker-compose", "override", "config", "console", "cli"],
    "homepage": "https://nickpoulos.info",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/nickpoulos/ez-rider/issues",
        "source": "https://github.com/nickpoulos/ez-rider"
    },
    "authors": [
        {
            "name": "Nick Poulos",
            "email": "nickpoulos.ny@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-yaml": "*",
        "composer/composer": "^2.1",
        "guzzlehttp/guzzle": "^7.4",
        "guzzlehttp/psr7": "^2.1",
        "illuminate/http": "^9.0",
        "laminas/laminas-text": "^2.8",
        "laravel-zero/phar-updater": "^1.0.6",
        "phpseclib/phpseclib": "~3.0",
        "wilderborn/partyline": "^1.0"
    },
    "require-dev": {
        "laravel-zero/framework": "^9.0",
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/ez-rider"]
}
